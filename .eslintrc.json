{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "implicit-arrow-linebreak": ["off"],
    "comma-dangle": ["off"],
    "consistent-return": ["off"],
    "no-console": ["Error"]
  }
}
